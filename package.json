{
  "name": "@rescapes/helpers-component",
  "version": "1.5.136",
  "description": "Helpers for working with React components",
  "main": "src/index.mjs",
  "module": "src/index.mjs",
  "type": "module",
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rescapes/helpers-component"
  },
  "author": "Andy Likuski andy@likuski.org",
  "bugs": {
    "url": "https://github.com/rescapes/helpers-component"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "build": "rollup -c",
    "prepare": "cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run build",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive"
  },
  "dependencies": {
    "@chakra-ui/core": "1.0.0-rc.8",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "apollo-client": "^2.6.4",
    "decamelize": "4.0.0",
    "folktale": "^2.1.0",
    "framer-motion": "^2.9.5",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^7.0.2",
    "prop-types": "^15.6.2",
    "ramda": "^0.27.1",
    "react-adopt": "^0.6.0",
    "react-apollo": "^3.0.1",
    "react-router-dom": "^5.2.0",
    "util": "^0.12.1"
  },
  "devDependencies": {
    "@rescapes/log": ">=0.1.30",
    "@rescapes/ramda": ">=1.6.169",
    "@rescapes/validate": ">=1.3.25",
    "@types/jest": "^26.0.15",
    "babelrc-rollup": "^3.0.0",
    "core-js": "^3.8.0",
    "cross-env": "^7.0.2",
    "dotenv": "8.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "enzyme-wait": "^1.0.9",
    "eslint": "7.14.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "graphql": ">=0.13.2",
    "install": "^0.13.0",
    "jest": "26.6.3",
    "jest-cli": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "jsdoc-babel": "^0.5.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "react": ">=16.4.1",
    "react-dev-utils": "^11.0.1",
    "react-dom": ">=16.4.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.34.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-watch": "4.3.1"
  },
  "peerDependencies": {
    "@rescapes/log": ">=0.1.21",
    "@rescapes/ramda": ">=1.5.78",
    "@rescapes/validate": ">=1.2.15"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jestsetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jest-environment-node",
    "testURL": "http://localhost",
    "transform": {},
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
