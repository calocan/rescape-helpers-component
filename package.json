{
  "name": "rescape-helpers-component",
  "version": "1.5.10",
  "description": "Helpers for working with React components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calocan/rescape-helpers-component"
  },
  "author": "Andy Likuski andy@likuski.org",
  "bugs": {
    "url": "https://github.com/calocan/rescape-helpers-component"
  },
  "license": "MIT",
  "scripts": {
    "install-peers": "cross-env node ./installPeersCommand.js | sh",
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=commonjs  node_modules/.bin/babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -out-dir dist",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production NODE_ENV=production rollup -c -out-dir dist",
    "build": "yarn run build:commonjs && yarn run build:es",
    "prepare": "yarn run clean && yarn run lint && yarn test && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g reactjs/redux && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:reactjs/redux gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn --new-version version patch && yarn publish",
    "publish-minor": "yarn --new-version version minor && yarn publish"
  },
  "dependencies": {
    "apollo-client": "^2.3.1",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-error": "^1.0.9",
    "apollo-link-schema": "^1.1.0",
    "apollo-test-utils": "^0.3.2",
    "data.either": "^1.5.1",
    "folktale": "^2.1.0",
    "graphql-tools": "^3.0.2",
    "longjohn": "^0.2.12",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "rebass": "^1.0.7",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "rescape-ramda": "^1.4.38",
    "rescape-validate": "^1.2.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "22.4.4",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "6.26.0",
    "cross-env": "^5.1.6",
    "dotenv": "5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "enzyme-wait": "^1.0.9",
    "eslint": "4.19.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.12.0",
    "install": "^0.11.0",
    "jest": "22.4.4",
    "jest-cli": "^22.4.4",
    "jsdoc-babel": "^0.4.0",
    "jsdom": "^11.11.0",
    "npm-install-peers": "^1.2.1",
    "react-dev-utils": "^5.0.1",
    "rollup": "^0.59.2",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.3.1"
  },
  "peerDependencies": {
    "graphql": "^0.13.1",
    "react": "^16.4.0",
    "react-dom": ">=0.14"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jestsetup.js",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "ie >= 11"
            ]
          },
          "exclude": [
            "transform-async-to-generator"
          ],
          "modules": false
        }
      ]
    ],
    "sourceMaps": true,
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "commonjs": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "node": "current"
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
