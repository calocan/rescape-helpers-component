{
  "name": "rescape-helpers-component",
  "version": "1.3.0",
  "description": "Helpers for working with React components",
  "main": "build/index.js",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "build-publish-patch": "npm run build && npm run test && npm version patch && npm run publish-patch",
    "build-publish-minor": "npm run build && npm run test && npm version minor && npm run publish-minor",
    "publish-patch": "npm version patch && npm publish",
    "publish-minor": "npm version minor && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calocan/rescape-helpers-component"
  },
  "author": "Andy Likuski andy@likuski.org",
  "bugs": {
    "url": "https://github.com/calocan/rescape-helpers-component"
  },
  "license": "MIT",
  "dependencies": {
    "apollo-client": "^2.2.0",
    "apollo-client-preset": "^1.0.6",
    "apollo-link-error": "^1.0.3",
    "apollo-link-schema": "^1.0.2",
    "apollo-test-utils": "^0.3.2",
    "data.either": "^1.5.1",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.0",
    "enzyme-wait": "^1.0.9",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.19.0",
    "jsdom": "^11.5.1",
    "longjohn": "^0.2.12",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux-mock-store": "^1.4.0",
    "redux-thunk": "^2.2.0",
    "rescape-ramda": "^1.4.8"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "dotenv": "4.0.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "jest": "20.0.4",
    "jest-cli": "^22.1.4",
    "react-dev-utils": "^4.2.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jestsetup.js",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "sourceMaps": true,
    "plugins": [
      [
        "transform-es2015-modules-commonjs-simple",
        {
          "noMangle": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
